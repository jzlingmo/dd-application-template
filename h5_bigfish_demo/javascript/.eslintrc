{
  "extends": "@alipay/bigfish/eslint",
  "rules": {
    "func-names": [0],
    "indent": ["error", 2],
    "no-tabs": [0],
    "no-empty-source": [0],
    "consistent-return": [0],
    "class-methods-use-this": [0],
    "no-param-reassign": [0],
    "max-len": [0],
    "import/no-unresolved": [0],
    "react/destructuring-assignment": [0],
    "react/prefer-stateless-function": [0],
    "react/jsx-no-bind": [0],
    "react/no-did-update-set-state": [1],
    "no-nested-ternary": [0],
    "no-unused-vars": [2],
    "arrow-parens": [
      "error",
      "as-needed"
    ]
  }
}